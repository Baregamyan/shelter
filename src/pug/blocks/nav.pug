nav.nav(aria-label=nav.aria.label aria-hidden="true")
  .nav__wrapper
    ul.nav__list
      each item in nav.list
        li.nav__item
          a.nav__link(class=data.href===item.href ? "nav__link--active" : null href=data.href===item.href ? null : data.main ? item.href : `/${item.href}`) #{item.name}
